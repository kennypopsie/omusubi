<h1>投稿の作成</h1>

<%= form_with(url: create_post_path, model: @post, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :body %>
    <%= form.text_area :body %>   
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
